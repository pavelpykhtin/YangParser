type string {
    length "1..max" {
        description "Dummy description";
        reference "Dummy reference";
        error-message "Dummy error message";
        error-app-tag "Dummy error app tag";
    }
    pattern '[a-zA-Z_][a-zA-Z0-9\-_.]*';
    pattern '[xX][mM][lL].*' {
        modifier invert-match;
        description "Dummy description";
        reference "Dummy reference";
        error-message "Dummy error message";
        error-app-tag "Dummy error app tag";
    }
}